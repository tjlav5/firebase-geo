{
  "name": "firebase-geo",
  "version": "0.1.0",
  "description": "",
  "main": "functions/lib/index.js",
  "scripts": {
    "start": "tsc --project ./functions && concurrently --kill-others \"npm:dev:emulators\" \"tsc --project ./functions --watch\"",
    "dev:functions": "cd functions && npm run dev",
    "dev:emulators": "firebase ext:dev:emulators:start --test-params=test_params.env --project=geo --test-config=firebase.json",
    "test": "firebase ext:dev:emulators:exec --test-params=test_params.env --project=geo-test --test-config=firebase.json `cd functions && npm run test-coverage`"
  },
  "author": "tjlav5",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "firebase-tools": "^8.9.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "ngeohash": "^0.6.3"
  }
}
